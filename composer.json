{
    "name": "thihasoehlaing/thai2d-crawler",
    "description": "crawl 2d number, set and value from set.or.th",
    "keywords": [
        "thai2d-crawler"
    ],
    "homepage": "https://github.com/Thihasoehlaing/Thai2D-Crawler",
    "license": "MIT",
    "authors": [
        {
            "name": ":Thiha Soe Hlaing",
            "email": "thihasoehlaing.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.16"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Thihasoehlaing\\TwoDCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thihasoehlaing\\TwoDCrawler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
